imports Kernel

scope MyvarOS

type Program
{
    @entry
    fn Main() 
    {                  
        Log("Starting Sedna Shell");

        display.clear();               

        Log("Sedna Shell Started");       

    }

    fn Log(msg : str)
    {   
        stdio.puts("[");
        display.setcolorfg(0xA);
        stdio.puts("Log");
        display.setcolorfg(0xF);
        stdio.puts("]");

        stdio.puts(msg);
    }
}