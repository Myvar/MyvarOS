imports Kernel

scope MyvarOS

type Program
{
    @entry
    fn Main() 
    {            
        dec num = 10 + 10 - 10 * 20 / 10;
        
        display.clear();

        display.setx(0);
        display.sety(1);

        display.setcolorfg(10);        

        stdio.puts("10 plus 10 is " + stdio.itos(num) + "\n");

        display.setcolorfg(0xF);

        Test();
    }

    fn Test()
    {
        stdio.puts("test");
    }
}